<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="Heartbeat">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Timestamp" type="xs:dateTime"/>
                <xs:element name="Status" type="xs:string"/>
                <xs:element name="SystemName" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
</xs:schema>